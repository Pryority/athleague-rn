{
  "expo": {
    "name": "athleague-rn",
    "slug": "athleague-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pryority.athleague"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.pryority.athleague"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["react-native-background-geolocation"],
    "extra": {
      "eas": {
        "projectId": "20bee62a-0976-4c10-bfcc-97f7d97d051b"
      }
    }
  }
}
